<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/" xmlns:search="http://www.sitecore.net/xmlconfig/search/" xmlns:environment="http://www.sitecore.net/xmlconfig/environment/">
  <sitecore>
    <pipelines>
      <metadata.getPageMetadata>
        <processor type="Xcms.Sitecore.Feature.Metadata.Infrastructure.Pipelines.GetPageMetadata.SetFromContext, Xcms.Sitecore.Feature.Metadata" resolve="true" />
        <processor type="Xcms.Sitecore.Feature.Metadata.Infrastructure.Pipelines.GetPageMetadata.SetFromPage, Xcms.Sitecore.Feature.Metadata" resolve="true" />
        <processor type="Xcms.Sitecore.Feature.Metadata.Infrastructure.Pipelines.GetPageMetadata.CombineTitle, Xcms.Sitecore.Feature.Metadata" resolve="true" />
        <processor type="Xcms.Sitecore.Feature.Metadata.Infrastructure.Pipelines.GetPageMetadata.CombineOpenGraph, Xcms.Sitecore.Feature.Metadata" resolve="true" />
      </metadata.getPageMetadata>
    </pipelines>
  </sitecore>
</configuration>