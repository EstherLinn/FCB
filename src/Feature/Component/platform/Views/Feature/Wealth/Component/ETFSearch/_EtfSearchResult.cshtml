@model Feature.Wealth.Component.Models.ETF.Search.EtfSearchResultModel

<div class="l-wrap">

    <div class="c-tab c-tab--wide" data-filter-empty="true" style="display:none;">
        <div class="c-tab__panel is-active">
            <div class="c-section">
                <div class="c-section__content">
                    <div class="c-empty">
                        <div class="c-empty__img">
                            <img src="~/themes/images/lions/lion-compare.svg" alt="">
                        </div>
                        <div class="c-empty__desc">查無商品，請嘗試調整篩選條件</div>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="c-tab c-tab--wide" data-tab="true" data-filter-empty="false" style="display:none;">
        <div class="c-tab__header">
            <ul class="c-tab__navs">
                <!-- .is-active: 作用中的項目 -->
                <!-- [data-tab-target]: 值需對應 [data-tab-panel-id] 的值，按下後會自動顯示相對的 panel -->
                <!-- 若為外連，不要賦予[data-tab-target]屬性 -->
                <li><a href="#" class="c-tab__item is-active" target="_self" title="市價績效" data-tab-target="tab-1" data-filter-switch-columns="marketPriceTable">市價績效</a></li>
                <li><a href="#" class="c-tab__item " target="_self" title="淨值績效" data-tab-target="tab-2" data-filter-switch-columns="netWorthTable">淨值績效</a></li>
                <li><a href="#" class="c-tab__item " target="_self" title="市價風險" data-tab-target="tab-3" data-filter-switch-columns="marketRiskTable" data-filter-switch="#uid-template-marketRisk">市價風險</a></li>
                <li><a href="#" class="c-tab__item " target="_self" title="淨值風險" data-tab-target="tab-4" data-filter-switch-columns="netWorthRiskTable" data-filter-switch="#uid-template-netWorthRisk">淨值風險</a></li>
                <li><a href="#" class="c-tab__item " target="_self" title="交易狀況" data-tab-target="tab-5" data-filter-switch-columns="tradeTable" data-filter-switch="#uid-template-trade">交易狀況</a></li>
                <li><a href="#" class="c-tab__item " target="_self" title="基本資料" data-tab-target="tab-6" data-filter-switch-columns="baseTable" data-filter-switch="#uid-template-base">基本資料</a></li>
                <li>@*<a href="#" class="c-tab__item " target="_self" title="查無商品" data-tab-target="tab-7" data-filter-empty="true">查無商品</a>*@</li>
            </ul>
            <div class="c-tab__collapse">
                <a href="#" class="c-tab__switch"></a>
                <div class="c-tab__dropdown">
                    <a href="#" class="c-tab__clone is-active" target="_self" title="市價績效" data-tab-target="tab-1" data-filter-switch-columns="1">市價績效</a>
                    <a href="#" class="c-tab__clone " target="_self" title="淨值績效" data-tab-target="tab-2" data-filter-switch-columns="2">淨值績效</a>
                    <a href="#" class="c-tab__clone " target="_self" title="市價風險" data-tab-target="tab-3" data-filter-switch-columns="3" data-filter-switch="#uid-template-marketRisk">市價風險</a>
                    <a href="#" class="c-tab__clone " target="_self" title="淨值風險" data-tab-target="tab-4" data-filter-switch-columns="4" data-filter-switch="#uid-template-netWorthRisk">淨值風險</a>
                    <a href="#" class="c-tab__clone " target="_self" title="交易狀況" data-tab-target="tab-5" data-filter-switch-columns="5" data-filter-switch="#uid-template-trade">交易狀況</a>
                    <a href="#" class="c-tab__clone " target="_self" title="基本資料" data-tab-target="tab-6" data-filter-switch-columns="6" data-filter-switch="#uid-template-base">基本資料</a>
                    @*<a href="#" class="c-tab__clone " target="_self" title="查無商品" data-tab-target="tab-7" data-filter-empty="true">查無商品</a>*@
                </div>
            </div>
        </div>
        <!-- is-active: 作用中的項目 -->

        <div class="c-tab__panel is-active" data-tab-panel-id="tab-1">
            <div class="l-flex u-flex-col u-flex-gap-sm">
