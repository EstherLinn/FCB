                                                                        {
                                                                            string distribution = string.Empty;
                                                                            string value = distribution;
                                                                            string display = string.Empty;

                                                                            if (frequency == null)
                                                                            {
                                                                                display = "display: none;";
                                                                                value = string.Empty;;
                                                                                distribution = string.Empty;;
                                                                            }
                                                                            else
                                                                            {
                                                                                value = frequency;
                                                                                distribution = frequency == "無" ? "不配息" : frequency;
                                                                            }

                                                                            <div class="l-pickGroup__item" style="@display">
                                                                                <label class="o-btnCheckBox" data-filter-checkbox>
                                                                                    <input class="o-btnCheckBox__input" type="checkbox" value="@value">
                                                                                    <div class="o-btnCheckBox__text" data-filter-value="@distribution"></div>
