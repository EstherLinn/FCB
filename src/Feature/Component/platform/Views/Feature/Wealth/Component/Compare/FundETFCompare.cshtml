            <% } %>
        </td>
        <td>
            <%switch (risk)
            {
            case 'RR1':
            risk = risk + ' 低度風險';
            break;
            case 'RR2':
            risk = risk + ' 中低度風險';
            break;
            case 'RR3':
            risk = risk + ' 中度風險';
            break;
            case 'RR4':
            risk = risk + ' 中高度風險';
            break;
            case 'RR5':
            risk = risk + ' 高度風險';
            break;
            } %>
            <%= risk ? risk: '-' %>
