        <td>
            <%= sharpe ? sharpe: '-' %>
        </td>
        <td>
            <%= beta ? beta: '-' %>
        </td>
        <td>
            <%= alpha ? alpha: '-' %>
        </td>
        <td>
            <%= deviation ? deviation + '%': '-' %>
        </td>
    </tr>
