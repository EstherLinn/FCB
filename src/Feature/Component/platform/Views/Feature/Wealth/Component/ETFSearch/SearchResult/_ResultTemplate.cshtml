@model Feature.Wealth.Component.Models.ETF.Search.EtfSearchResultModel

<script id="uid-template" type="text/html">
    <tr class="c-cardTable__item u-nowrap@dt-only u-center@dt-only">
        <!-- .c-cardTable__top: 若需轉換成卡片，且有上方 header 色塊區，則需要放置此一欄位在 tr下的第一個欄位，無 header 就不需要此欄位 -->
        <td class="c-cardTable__top"></td>
        <!-- .c-cardTable__header: 轉成卡片時若為 header 區塊，需要加此 class name -->
        <!-- .u-wrap@dt-only: 桌機版(@@desktop)自動折行 -->
        @*【ETF名稱】=> 所有tab *@
        <td class="c-cardTable__header u-wrap@dt-only u-left@dt-only" data-table-name="marketPriceTable,netWorthTable,marketRiskTable,netWorthRiskTable,tradeTable,baseTable">
            <div class="c-cardTable__content">
                <div class="l-target">
                    <div class="l-target__name">
                        <a href="@(Model.DetailPageLink)?id=<%= FirstBankCode %>" class="o-contentLink t-bold u-ellipsis-2" target="_blank"><%= FirstBankCode %> <%= ETFName %></a>
                    </div>
                    @*標籤*@
                    <% if (DiscountTags.length > 0) { %>
                    <div class="l-target__additional">
                        <ul class="l-tag">
                            <% for (var i = 0; i < DiscountTags.length; i++) { %>
                            <li class="l-tag__item">
                                @*<a href="#" class="o-tag"><%= DiscountTags[i] %></a>*@
                                <span class="o-tag"><%= DiscountTags[i] %></span>
                            </li>
                            <% } %>
                        </ul>
                    </div>
                    <% } %>
                </div>
            </div>
        </td>
        <!-- .c-cardTable__space: 若需轉換成卡片，需要在 body 區塊前額外放置此一欄位分隔 -->
        <td class="c-cardTable__space"></td>
        <!-- .c-cardTable__body: 轉成卡片時若為 body 區塊，需要加此 class name -->
        <!-- [data-card-table-width]: 設定轉換成卡片時的欄位寬度，值為"幾分之幾"，範圍1/2 ~ 3/4 -->
        @*【交易所代碼】=> 所有tab *@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketPriceTable,netWorthTable,marketRiskTable,netWorthRiskTable,tradeTable,baseTable">
            <!-- [data-length]: 設定轉換成卡片時，標題的寬度，單位為字數em，範圍1~5字 -->
            <div class="c-cardTable__title" data-length="5">交易所代碼</div>
            <div class="c-cardTable__content"><%= ExchangeCode.Text %></div>
        </td>
        @*【市價(日期)】=> 市價績效,市價風險,交易狀況,基本資料*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketPriceTable,marketRiskTable,tradeTable,baseTable">
            <!-- [data-length]: 設定轉換成卡片時，標題的寬度，單位為字數em，範圍1~5字 -->
            <div class="c-cardTable__title" data-length="4">市價<br>(日期)</div>
            <div class="c-cardTable__content"><%= MarketPrice.Text %><br><span class="t-date"><%= MarketPriceDate %></span></div>
        </td>
        @*【最新淨值(日期)】=> 淨值績效,淨值風險*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="netWorthTable,netWorthRiskTable">
            <!-- [data-length]: 設定轉換成卡片時，標題的寬度，單位為字數em，範圍1~5字 -->
            <div class="c-cardTable__title" data-length="4">最新淨值<br>(日期)</div>
            <div class="c-cardTable__content"><%= NetAssetValue.Text %><br><span class="t-date"><%= NetAssetValueDate %></span></div>
        </td>
        @*【幣別】=> 市價績效,淨值績效,基本資料*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketPriceTable,netWorthTable,baseTable">
            <div class="c-cardTable__title" data-length="5">
                幣別
                <a href="#popupCurrency" class="o-icon o-icon--tip o-icon--xs" data-popup="true"></a>
            </div>
            <div class="c-cardTable__content">
                <a href="#popupCurrencyWebSite" class="o-link" data-popup="true"><%= CurrencyPair.Value %></a>
            </div>
        </td>
        @*【市價績效-報酬(原幣)】=> 市價績效*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketPriceTable" data-table-column-currency="Original">
            <div class="c-cardTable__title" data-length="4">1個月</div>
            <div class="c-cardTable__content">
                <span class="<%= MonthlyReturnMarketPriceOriginalCurrency.Style %>"><%= MonthlyReturnMarketPriceOriginalCurrency.Text %></span>
            </div>
        </td>
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketPriceTable" data-table-column-currency="Original">
            <div class="c-cardTable__title" data-length="5">3個月</div>
            <div class="c-cardTable__content">
                <span class="<%= ThreeMonthReturnMarketPriceOriginalCurrency.Style %>"><%= ThreeMonthReturnMarketPriceOriginalCurrency.Text %></span>
            </div>
        </td>
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketPriceTable" data-table-column-currency="Original">
            <div class="c-cardTable__title" data-length="4">6個月</div>
            <div class="c-cardTable__content">
                <span class="<%= SixMonthReturnMarketPriceOriginalCurrency.Style %>"><%= SixMonthReturnMarketPriceOriginalCurrency.Text %></span>
            </div>
        </td>
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketPriceTable" data-table-column-currency="Original">
            <div class="c-cardTable__title" data-length="5">1年</div>
            <div class="c-cardTable__content">
                <span class="<%= OneYearReturnMarketPriceOriginalCurrency.Style %>"><%= OneYearReturnMarketPriceOriginalCurrency.Text %></span>
            </div>
        </td>
        @*【市價績效-報酬(台幣)】=> 市價績效*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketPriceTable" data-table-column-currency="TWD">
            <div class="c-cardTable__title" data-length="4">1個月</div>
            <div class="c-cardTable__content">
                <span class="<%= MonthlyReturnMarketPriceTWD.Style %>"><%= MonthlyReturnMarketPriceTWD.Text %></span>
            </div>
        </td>
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketPriceTable" data-table-column-currency="TWD">
            <div class="c-cardTable__title" data-length="5">3個月</div>
            <div class="c-cardTable__content">
                <span class="<%= ThreeMonthReturnMarketPriceTWD.Style %>"><%= ThreeMonthReturnMarketPriceTWD.Text %></span>
            </div>
        </td>
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketPriceTable" data-table-column-currency="TWD">
            <div class="c-cardTable__title" data-length="4">6個月</div>
            <div class="c-cardTable__content">
                <span class="<%= SixMonthReturnMarketPriceTWD.Style %>"><%= SixMonthReturnMarketPriceTWD.Text %></span>
            </div>
        </td>
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketPriceTable" data-table-column-currency="TWD">
            <div class="c-cardTable__title" data-length="5">1年</div>
            <div class="c-cardTable__content">
                <span class="<%= OneYearReturnMarketPriceOriginalCurrency.Style %>"><%= OneYearReturnMarketPriceOriginalCurrency.Text %></span>
            </div>
        </td>
        @*【淨值績效-報酬(原幣)】=> 淨值績效*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="netWorthTable" data-table-column-currency="Original">
            <div class="c-cardTable__title" data-length="4">1個月</div>
            <div class="c-cardTable__content">
                <span class="<%= MonthlyReturnNetValueOriginalCurrency.Style %>"><%= MonthlyReturnNetValueOriginalCurrency.Text %></span>
            </div>
        </td>
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="netWorthTable" data-table-column-currency="Original">
            <div class="c-cardTable__title" data-length="5">3個月</div>
            <div class="c-cardTable__content">
                <span class="<%= ThreeMonthReturnNetValueOriginalCurrency.Style %>"><%= ThreeMonthReturnNetValueOriginalCurrency.Text %></span>
            </div>
        </td>
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="netWorthTable" data-table-column-currency="Original">
            <div class="c-cardTable__title" data-length="4">6個月</div>
            <div class="c-cardTable__content">
                <span class="<%= SixMonthReturnNetValueOriginalCurrency.Style %>"><%= SixMonthReturnNetValueOriginalCurrency.Text %></span>
            </div>
        </td>
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="netWorthTable" data-table-column-currency="Original">
            <div class="c-cardTable__title" data-length="5">1年</div>
            <div class="c-cardTable__content">
                <span class="<%= OneYearReturnNetValueOriginalCurrency.Style %>"><%= OneYearReturnNetValueOriginalCurrency.Text %></span>
            </div>
        </td>

        @*【淨值績效-報酬(台幣)】=> 淨值績效*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="netWorthTable" data-table-column-currency="TWD">
            <div class="c-cardTable__title" data-length="4">1個月</div>
            <div class="c-cardTable__content">
                <span class="<%= MonthlyReturnNetValueTWD.Style %>"><%= MonthlyReturnNetValueTWD.Text %></span>
            </div>
        </td>
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="netWorthTable" data-table-column-currency="TWD">
            <div class="c-cardTable__title" data-length="5">3個月</div>
            <div class="c-cardTable__content">
                <span class="<%= ThreeMonthReturnNetValueTWD.Style %>"><%= ThreeMonthReturnNetValueTWD.Text %></span>
            </div>
        </td>
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="netWorthTable" data-table-column-currency="TWD">
            <div class="c-cardTable__title" data-length="4">6個月</div>
            <div class="c-cardTable__content">
                <span class="<%= SixMonthReturnNetValueTWD.Style %>"><%= SixMonthReturnNetValueTWD.Text %></span>
            </div>
        </td>
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="netWorthTable" data-table-column-currency="TWD">
            <div class="c-cardTable__title" data-length="5">1年</div>
            <div class="c-cardTable__content">
                <span class="<%= OneYearReturnNetValueTWD.Style %>"><%= OneYearReturnNetValueTWD.Text %></span>
            </div>
        </td>
        @*【風險屬性】=> 市價風險,淨值風險*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketRiskTable,netWorthRiskTable">
            <div class="c-cardTable__title" data-length="5">風險屬性</div>
            <div class="c-cardTable__content"><%= RiskLevel.Text %></div>
        </td>
        @*【Sharpe】=> 市價風險*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketRiskTable">
            <div class="c-cardTable__title" data-length="5">Sharpe</div>
            <div class="c-cardTable__content"><%= SharpeRatioMarketPriceRisk.Text %></div>
        </td>
        @*【Beta】=> 市價風險*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketRiskTable">
            <div class="c-cardTable__title" data-length="5">Beta</div>
            <div class="c-cardTable__content"><%= BetaMarketPriceRisk.Text %></div>
        </td>
        @*【年化標準差】=> 市價風險*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="marketRiskTable">
            <div class="c-cardTable__title" data-length="5">年化標準差</div>
            <div class="c-cardTable__content"><%= AnnualizedStandardDeviationMarketPriceRisk.Text %></div>
        </td>
        @*【Sharpe】=> 淨值風險*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="netWorthRiskTable">
            <div class="c-cardTable__title" data-length="5">Sharpe</div>
            <div class="c-cardTable__content"><%= SharpeNetValueRisk.Text %></div>
        </td>
        @*【Beta】=> 淨值風險*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="netWorthRiskTable">
            <div class="c-cardTable__title" data-length="5">Beta</div>
            <div class="c-cardTable__content"><%= BetaNetValueRisk.Text %></div>
        </td>
        @*【年化標準差】=> 淨值風險*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="netWorthRiskTable">
            <div class="c-cardTable__title" data-length="5">年化標準差</div>
            <div class="c-cardTable__content"><%= AnnualizedStandardDeviationNetValueRisk.Text %></div>
        </td>
        @*【折溢價】=> 交易狀況*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="tradeTable">
            <div class="c-cardTable__title" data-length="5">折溢價</div>
            <span class="<%= DiscountPremium.Style %>"><%= DiscountPremium.Text %></span>
        </td>
        @*【最新量】=> 交易狀況*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="tradeTable">
            <div class="c-cardTable__title" data-length="4">最新量</div>
            <div class="c-cardTable__content"><%= LatestVolumeTradingVolume.Text %></div>
        </td>
        @*【最新量-10日量】=> 交易狀況*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="tradeTable">
            <div class="c-cardTable__title" data-length="5">最新量-10日量</div>
            <div class="c-cardTable__content"><%= LatestVolumeTradingVolumeTenDayAverageVolume.Text %></div>
        </td>
        @*【投資標的】=> 基本資料*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="baseTable">
            <div class="c-cardTable__title" data-length="4">投資標的</div>
            <div class="c-cardTable__content"><%= InvestmentTarget.Text %></div>
        </td>
        @*【成立年資】=> 基本資料*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="baseTable">
            <div class="c-cardTable__title" data-length="5">成立年資</div>
            <div class="c-cardTable__content"><%= EstablishmentSeniority.Text %></div>
        </td>
        @*【管理費】=> 基本資料*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="baseTable">
            <div class="c-cardTable__title" data-length="4">管理費</div>
            <div class="c-cardTable__content"><%= TotalManagementFee.Text %></div>
        </td>
        @*【規模(百萬)】=> 基本資料*@
        <td class="c-cardTable__body" data-card-table-width="1/2" data-table-name="baseTable">
            <div class="c-cardTable__title" data-length="5">規模<br>(百萬)</div>
            <div class="c-cardTable__content"><%= ScaleMillions.Text %></div>
        </td>

        <!-- .c-cardTable__space: 若需轉換成卡片，body 區塊結尾處需要額外放置此一欄位分隔 -->
        <td class="c-cardTable__space"></td>
        <!-- .c-cardTable__footer: 轉成卡片時若為下方按鈕區塊，需要加此 class name -->
        <td class="c-cardTable__footer" data-card-table-width="<%= CanOnlineSubscription == true ? '2/3' : '1/1' %>">
            <div class="c-cardTable__content">
                <ul class="l-collect l-collect--divider@lt">
                    <li class="l-collect__item">
                        <!-- [data-after-lt]: pad / mobile 自動顯示文字在後方，值為要顯示的文字 -->
                        <!-- [data-ia="true"]: 啟用互動式按鈕，預設會有 tooltip 及 toast 提示訊息，需搭配 [data-msg] 設定訊息才會有作用 -->
                        <!-- [data-msg]: active|inactive 的訊息，用 "|" 符號分隔，若訊息未設定完整則不顯示提示訊息 -->
                        <a href="#" class="o-statusBtn o-statusBtn--like" data-after-lt="關注" data-msg="加入關注|取消關注" data-ia="true"></a>
                    </li>
                    <li class="l-collect__item">
                        <!-- [data-after-lt]: pad / mobile 自動顯示文字在後方，值為要顯示的文字 -->
                        <!-- [data-ia="true"]: 啟用互動式按鈕，預設會有 tooltip 及 toast 提示訊息，需搭配 [data-msg] 設定訊息才會有作用 -->
                        <!-- [data-msg]: active|inactive 的訊息，用 "|" 符號分隔，若訊息未設定完整則不顯示提示訊息 -->
                        <a href="#" class="o-statusBtn o-statusBtn--compare" data-after-lt="比較" data-msg="加入比較|取消比較" data-ia="true"></a>
                    </li>
                </ul>
            </div>
        </td>
        <td class="c-cardTable__footer" data-card-table-width="1/3">
            <div class="c-cardTable__content">
                <% if(CanOnlineSubscription) { %>
                <a href="#popupPurchase" class="o-tableBtn o-tableBtn--card@lt" data-popup="true">申購</a>
                <% } %>
            </div>
        </td>
    </tr>
</script>

