                var token = $('input[name="__RequestVerificationToken"]').val();

                $.ajax({
                    url: '@ClientRoute.GenerateUrl(this, "Calculate", "GetFundData")',
                    method: 'POST',
                    data: {
                        ExpectedRoi: expectedRoi,
                        ProductFundIDs: fundIds,
                        __RequestVerificationToken: token
