<script>
    (function ($, window, document, undefined) {
        document.addEventListener("DOMContentLoaded", function () {
            var resultHasGap;
            var assetsData;
            var description, readingbar = [], earningsChart = [], chartsRevenues = [], chartsRewards = [], chartsRewardsCategories = [], chartsAssets = [];
            var hasRenderedRecommendations = false;
            var lockImg = "/themes/images/icons/others/lock.svg"
            var defaultRiskAttributes = '@Model.DefaultRiskAttributes';
            var fundIds = @(new HtmlString(JsonHelper.SerializeObject(Model.FundID)));
            var educationalFund, years, amountPrepared, monthlyInvestment, expectedRoi;
            var educationalFundValue, educationalFundyears, educationalFundamountPrepared, educationalFundmonthlyInvestment, educationalFundexpectedRoi;
            var SessionKeys = [
                'educationalFundValue',
                'educationalFundyears',
                'educationalFundamountPrepared',
                'educationalFundmonthlyInvestment',
                'educationalFundexpectedRoi'
            ];

            var conservativeAssetsData = {
                stock: {
                    number: @Model.ConservativeRiskStockAllocation,
                    fieldName: '@Model.ConservativeRiskStockAllocationFieldName',
                    text: '@Model.ConservativeRiskStockAllocationText'
                },
                bond: {
                    number: @Model.ConservativeRiskBondAllocation,
                    fieldName: '@Model.ConservativeRiskBondAllocationFieldName',
                    text: '@Model.ConservativeRiskBondAllocationText'
                },
                currency: {
                    number: @Model.ConservativeRiskCurrencyAllocation,
                    fieldName: '@Model.ConservativeRiskCurrencyAllocationFieldName',
                    text: '@Model.ConservativeRiskCurrencyAllocationText'
                }
            };

            var robustAssetsData = {
                stock: {
                    number: @Model.RobustRiskStockAllocation,
                    fieldName: '@Model.RobustRiskStockAllocationFieldName',
                    text: '@Model.RobustRiskStockAllocationText'
                },
                bond: {
                    number: @Model.RobustRiskBondAllocation,
                    fieldName: '@Model.RobustRiskBondAllocationFieldName',
                    text: '@Model.RobustRiskBondAllocationText'
                },
                currency: {
                    number: @Model.RobustRiskCurrencyAllocation,
                    fieldName: '@Model.RobustRiskCurrencyAllocationFieldName',
                    text: '@Model.RobustRiskCurrencyAllocationText'
                }
            };

            var positiveAssetsData = {
                stock: {
                    number: @Model.PositiveRiskStockAllocation,
                    fieldName: '@Model.PositiveRiskStockAllocationFieldName',
                    text: '@Model.PositiveRiskStockAllocationText'
                },
                bond: {
                    number: @Model.PositiveRiskBondAllocation,
                    fieldName: '@Model.PositiveRiskBondAllocationFieldName',
                    text: '@Model.PositiveRiskBondAllocationText'
                },
                currency: {
                    number: @Model.PositiveRiskCurrencyAllocation,
                    fieldName: '@Model.PositiveRiskCurrencyAllocationFieldName',
                    text: '@Model.PositiveRiskCurrencyAllocationText'
                }
            };

            //#region 更新風險屬性相關內容&判斷對應遮罩事件
            handleRisk();

            if (SC_DATA.Risk == '1') {
                assetsData = conservativeAssetsData;
            } else if (SC_DATA.Risk == '2') {
                assetsData = robustAssetsData;
            } else if (SC_DATA.Risk == '3') {
                assetsData = positiveAssetsData;
            } else {
                if (defaultRiskAttributes == '2') {
                    assetsData = conservativeAssetsData;
                    UpdateRiskAttributesContent('1');
                } else if (defaultRiskAttributes == '5') {
                    assetsData = robustAssetsData;
                    UpdateRiskAttributesContent('2');
                } else if (defaultRiskAttributes == '7') {
                    assetsData = positiveAssetsData;
                    UpdateRiskAttributesContent('3');
                }
            }
            //#endregion

            //#region 更新風險屬性&判斷對應遮罩方法
            async function handleRisk() {
                if (SC_DATA.IsLoggedIn) {
                    $('[data-save="true"]').removeClass('u-hidden');

                    if (SC_DATA.Risk === undefined) {
                        try {
                            const data = await RiskHandler();
                            SC_DATA.Risk = data.risk;
                        } catch (error) {
                            console.error('Error fetching risk data:', error);
                        }
                    }

                    if (SC_DATA.Risk !== undefined && SC_DATA.PlatForm === 'WebBank') {
                        UpdateRiskAttributesContent(SC_DATA.Risk);
                    } else if (SC_DATA.Risk === undefined && SC_DATA.PlatForm === 'WebBank') {
                        renderNotRisk();
                    }
                } else {
                    renderNotLogin();
                }
            }
            //#endregion

            //#region 預計幾年後達成目標綁定事件
            let $sliderYear = $('[data-slider-id="year"]').get(0),
                $inputYear = $('[data-id="year"]');
            noUiSlider.create($sliderYear, {
                start: 10,
                connect: [true, false],
                step: 1,
                range: {
                    'min': [1, 1],
                    'max': 30
                },
                format: wNumb({
                    decimals: 0
                }),
                pips: {
                    mode: 'positions',
                    values: [0, 100],
                    density: -1,
                    format: wNumb({
                        decimals: 0,
                        suffix: '年'
                    })
                }
            });
            $sliderYear.noUiSlider.on('update', function (values, handle) {
                const year = Number(values[handle]);
