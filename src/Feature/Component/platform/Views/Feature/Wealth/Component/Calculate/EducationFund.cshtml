                    success: function (data) {
                        if (data != null && data.length != 0) {

                            $(`#${tab}Fund`).removeClass('u-hidden');
                            RealrenderRecommendedFunds(tab, event, data);
                            FundinitializeSwiper(eventString);
                        } else {
