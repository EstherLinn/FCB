            //#region Div移除屬性
            function divRemoveAttributes($div) {
                if (!$div || !$div.jquery || $div.length === 0) {
                    return;
                }

                // 獲取除 'id' 之外的所有屬性名稱
                var attributes = $.map($div[0].attributes, function (attr) {
                    return attr.name !== 'id' ? attr.name : null;
                });

                // 移除所有非 'id' 屬性
                $div.removeAttr(attributes.join(' '));
            }
