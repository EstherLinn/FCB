<script>
    $(function () {
        $('.o-pelletbox__input').each(function (i, el) {
            var val = $(el).val();

            if (val == '亞洲' || val == '美洲' || val == '歐非中東') {
                $(el).on("click", function () {
                    $('#亞洲').hide();
                    $('#美洲').hide();
                    $('#歐非中東').hide();

                    $('#' + $(this).val()).show();
                });
            }
            else if (val == '能源' || val == '貴金屬' || val == '農作物') {
                $(el).on("click", function () {
                    $('#能源').hide();
                    $('#貴金屬').hide();
                    $('#農作物').hide();

                    $('#' + $(this).val()).show();
                });
            }
            else if (val == '各國債券利率' || val == '債券指數行情') {
                $(el).on("click", function () {
                    $('#各國債券利率').hide();
                    $('#債券指數行情').hide();

                    $('#' + $(this).val()).show();
                });
            }
            else if (val == '主要匯率' || val == '亞州匯率' || val == '歐非中東匯率') {
                $(el).on("click", function () {
                    $('#主要匯率').hide();
                    $('#亞州匯率').hide();
                    $('#歐非中東匯率').hide();

                    $('#' + $(this).val()).show();
                });
            }
        });
    });
</script>