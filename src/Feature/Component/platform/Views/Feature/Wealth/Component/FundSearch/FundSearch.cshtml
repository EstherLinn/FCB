						template: '#uid-template',
						callbacks: {
							afterFilter: function (result) {
								window.loading('hide');
								$('[id="uid-total"]').text(result.length);
							}
						},
						search: { ele: '#uid-searchbox' },
						order: {
							default_sort: 'asc',
