@using Foundation.Wealth.Helper;
<script>
        let xhr = new XMLHttpRequest();
        xhr.open('post', '@ClientRoute.GenerateUrl(this, "Accounts", "GetUrlCookie")', true);
        xhr.send(null);
        xhr.onload = function () {
            let res = JSON.parse(xhr.responseText);
            if (res.url == '') {
                           location.href = '/';
            }else{
            const url = new URL(res.url);
           location.href = url;
            }
        }
</script>